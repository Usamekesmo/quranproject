<span class="line"><span style="color:#A626A4">        const</span><span style="color:#986801"> newTitles</span><span style="color:#0184BC"> =</span><span style="color:#383A42"> player.</span><span style="color:#E45649">titles</span><span style="color:#0184BC"> ?</span><span style="color:#383A42"> [</span><span style="color:#0184BC">...</span><span style="color:#383A42">player.</span><span style="color:#C18401">titles</span><span style="color:#383A42">, rewardInfo.</span><span style="color:#C18401">title_reward</span><span style="color:#383A42">] </span><span style="color:#0184BC">:</span><span style="color:#383A42"> [rewardInfo.</span><span style="color:#C18401">title_reward</span><span style="color:#383A42">];</span></span>
