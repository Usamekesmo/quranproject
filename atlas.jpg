<span class="line"></span>
<span class="line"><span style="color:#A626A4">export</span><span style="color:#A626A4"> function</span><span style="color:#4078F2"> displayLeaderboard</span><span style="color:#383A42">(leaderboardData, key </span><span style="color:#0184BC">=</span><span style="color:#50A14F"> 'xp'</span><span style="color:#383A42">) {</span></span>
<span class="line"><span style="color:#A626A4">    if</span><span style="color:#383A42"> (</span><span style="color:#0184BC">!</span><span style="color:#383A42">leaderboardList) </span><span style="color:#A626A4">return</span><span style="color:#383A42">;</span></span>
<span class="line"><span style="color:#383A42">    leaderboardList.</span><span style="color:#E45649">innerHTML</span><span style="color:#0184BC"> =</span><span style="color:#50A14F"> ''</span><span style="color:#383A42">;</span></span>
<span class="line"><span style="color:#A626A4">    if</span><span style="color:#383A42"> (</span><span style="color:#0184BC">!</span><span style="color:#383A42">leaderboardData </span><span style="color:#0184BC">||</span><span style="color:#383A42"> leaderboardData.</span><span style="color:#E45649">length</span><span style="color:#0184BC"> ===</span><span style="color:#986801"> 0</span><span style="color:#383A42">) {</span></span>
<span class="line"><span style="color:#383A42">        leaderboardList.</span><span style="color:#E45649">innerHTML</span><span style="color:#0184BC"> =</span><span style="color:#50A14F"> '&lt;p&gt;لا توجد بيانات لعرضها حاليًا.&lt;/p&gt;'</span><span style="color:#383A42">;</span></span>
