<span class="line"><span style="color:#0184BC">            :</span><span style="color:#383A42"> player.</span><span style="color:#E45649">username</span><span style="color:#383A42">;</span></span>
<span class="line"></span>
<span class="line"><span style="color:#383A42">        item.</span><span style="color:#E45649">innerHTML</span><span style="color:#0184BC"> =</span><span style="color:#50A14F"> `&lt;span class="leaderboard-rank"&gt;</span><span style="color:#CA1243">${</span><span style="color:#383A42">index</span><span style="color:#0184BC"> +</span><span style="color:#986801"> 1</span><span style="color:#CA1243">}</span><span style="color:#50A14F">&lt;/span&gt;&lt;span class="leaderboard-name"&gt;</span><span style="color:#CA1243">${</span><span style="color:#383A42">displayName</span><span style="color:#CA1243">}</span><span style="color:#50A14F">&lt;/span&gt;&lt;span class="leaderboard-xp"&gt;</span><span style="color:#CA1243">${</span><span style="color:#383A42">player[key]</span><span style="color:#CA1243">}</span><span style="color:#50A14F"> XP&lt;/span&gt;`</span><span style="color:#383A42">;</span></span>
