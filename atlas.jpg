<span class="line"><span style="color:#383A42">          .</span><span style="color:#4078F2">from</span><span style="color:#383A42">(</span><span style="color:#50A14F">'players'</span><span style="color:#383A42">)</span></span>
<span class="line"><span style="color:#383A42">          .</span><span style="color:#4078F2">update</span><span style="color:#383A42">({</span></span>
