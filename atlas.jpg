<span class="line"><span style="color:#E45649">      headers</span><span style="color:#0184BC">:</span><span style="color:#383A42"> { </span><span style="color:#0184BC">...</span><span style="color:#383A42">corsHeaders, </span><span style="color:#50A14F">'Content-Type'</span><span style="color:#0184BC">:</span><span style="color:#50A14F"> 'application/json'</span><span style="color:#383A42"> },</span></span>
<span class="line"><span style="color:#E45649">      status</span><span style="color:#0184BC">:</span><span style="color:#986801"> 500</span><span style="color:#383A42">,</span></span>
