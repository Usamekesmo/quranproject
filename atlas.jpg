<span class="line"><span style="color:#383A42">      .</span><span style="color:#4078F2">update</span><span style="color:#383A42">({ </span><span style="color:#E45649">is_active</span><span style="color:#0184BC">:</span><span style="color:#986801"> false</span><span style="color:#383A42"> })</span></span>
<span class="line"><span style="color:#383A42">      .</span><span style="color:#4078F2">eq</span><span style="color:#383A42">(</span><span style="color:#50A14F">'id'</span><span style="color:#383A42">, season.</span><span style="color:#E45649">id</span><span style="color:#383A42">);</span></span>
